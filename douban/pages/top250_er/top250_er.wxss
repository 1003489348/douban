.wrap{
  width: 100%;
  padding-top: 40rpx;
  background-color: #fff8f8;
}
.xq_title{
  width: 90%;
  margin: 0 auto;
  overflow: hidden;
}
.xq_title image{
  width: 30%;
  border-radius: 10rpx;
  float: left;
}
.xq_title_text{
  width: 65%;
  float: left;
  color: rgb(194, 194, 194);
  /* font-size: 24rpx; */
  margin-left: 30rpx;
}
.xq_title_text text{
  display: block;
}
.xq_title_text text:nth-child(1){
  color: #111;
  font-size: 40rpx;
  font-weight: 500;
}
.xq_title_text text:nth-child(2){
  font-size: 28rpx;
}
.xq_title_text text:nth-child(3){
  font-size: 24rpx;
}
.xq_link{
  float: left;
  margin-left: 30rpx;
  overflow: hidden;
  margin-top: 30rpx;
}
.xq_link .xq_link_1, .xq_link .xq_link_2{
  width: 170rpx;
  height: 65rpx;
  line-height: 65rpx;
  display: inline-block;
  text-align: center;
  background-color: #fff;
  border-radius: 10rpx;
  font-size: 28rpx;
}
.xq_link .xq_link_1{
  margin-right: 30rpx;
  /* box-shadow: 1px 1px 0px #ccc; */
   border-bottom: 1px solid rgb(150, 150, 150);
}
.xq_link .xq_link_2{
  /* box-shadow: -1px -1px 10px #ccc; */
  border-bottom: 1px solid rgb(150, 150, 150);
}
.xq_pf{
  width: 90%;
  margin: 50rpx auto 0;
  background-color: #fff;
  border-radius: 10rpx;
  box-sizing: border-box;
  padding:  30rpx 30rpx 60rpx 30rpx;
  overflow: hidden;
}
.xq_pf .xq_pf_title{
  /* color: #222; */
  font-size: 28rpx;
  font-weight: 550;
  width: 100%;
  overflow: hidden;
}
.xq_pf .xq_pf_title text:nth-child(2){
  font-size: 15rpx;
  position: relative;
  top: -20rpx;
}
.xq_pf .xq_pf_title text:nth-child(3){
  float: right;
}
.xq_pf_start{
  width: 25%;
  margin-left: 18%;
  text-align: center;
  float: left;
}
.xq_pf_start text{
  font-size: 50rpx;
  font-weight: 600;
  text-align: center;
  color: #111;
}
.xq_pf_r{
  width: 20%;
  text-align: right;
  float: left;
  /* margin-top: 15rpx; */
}
.shuzhutu{
  width: 160rpx;
  height: 80rpx;
  float: left;
  /* border: 1px solid red; */
  margin-left: 15rpx;
}
.shuzhutu1{
  width: 100%;
  height: 13rpx;
  border-radius: 8rpx;
  margin-bottom: 5rpx;
  background-color: rgb(224, 224, 224);
}
.shuzhutu1:nth-child(1) view{
  width: 80%;
  height: 100%;
  background-color: #f90;
  border-radius: 8rpx;
}
.shuzhutu1:nth-child(2) view{
  width: 40%;
  height: 100%;
  background-color: #f90;
  border-radius: 8rpx;
}
.shuzhutu1:nth-child(3) view{
  width: 10%;
  height: 100%;
  background-color: #f90;
  border-radius: 8rpx;
}
.shuzhutu text{
  font-size: 20rpx;
  color: rgb(224, 224, 224);
}
.xq_jj{
  width: 90%;
  margin: 70rpx auto 0;
}
.xq_jj text:nth-child(1){
  display: block;
  color: #333;
  font-size: 32rpx;
  font-weight: 800;
}
.xq_jj text:nth-child(2){
  display: inline-block;
  /* height: 180rpx; */
  /* white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis; */
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  width: 100%;
  margin-top: 30rpx;
  font-size: 28rpx;
  color: #333;
}
/* 文本显示 */
.xq_jj .show{
  -webkit-line-clamp: 0;
}
/* 文本隐藏 */
.xq_jj .hide{
  -webkit-line-clamp: 4;
}
.xq_jj text:nth-child(3){
  display: inline-block;
  width: 100%;
  font-size: 32rpx;
  color: rgb(177, 177, 177);
  text-align: right;
}
.xq_yingren{
  width: 90%;
  margin: 70rpx auto 0;
}
.xq_yingren text:nth-child(1){
  display: block;
  color: #333;
  font-size: 32rpx;
  font-weight: 800;
  margin-bottom: 30rpx;
}
/* 图片滑动 */
.uploadWrap {
  /* float: left; */
  display: flex;
  flex-direction: row;
  width: 100%;
  height: 330rpx;
}
.upload_Item {
  /* float: left; */
  width: 200rpx;
  height: 330rpx;
  line-height: 40rpx;
  text-align: center;
}
.upload_Item_img {
  /* float: left; */
  width: 170rpx;
  height: 250rpx;
  margin: 0 20rpx 0 0;
  border-radius: 10rpx;
}
.upload_Item text {
  display: inline-block;
  white-space:nowrap;
  overflow:hidden;
  text-overflow:ellipsis;
  width: 160rpx;
  font-size: 26rpx; 
  text-align: left;
}
.baozhao {
  width: 90%;
  margin: 70rpx auto 0rpx;
}
.baozhao text:nth-child(1){
  display: block;
  color: #333;
  font-size: 32rpx;
  font-weight: 800;
}
.upload_Item video{
  height: 328rpx;
}
.dp {
  width: 90%;
  margin: 70rpx auto 0rpx;
  background-color: #fff;
  border-radius: 20rpx;
  padding: 30rpx;
  box-sizing: border-box;
}
.dp text:nth-child(1){
  display: block;
  color: #333;
  font-size: 32rpx;
  font-weight: 800;
}
.dp_title{
  width: 100%;
  overflow: hidden;
  margin-top: 30rpx;
  border-bottom: 1px solid rgb(223, 223, 223);
}
.dp_title image{
  width: 70rpx;
  height: 70rpx;
  border-radius: 50%;
  float: left;
}
.dp_gr{
  margin-left: 30rpx;
  float: left;
}
.dp_time{
  font-size: 22rpx;
  color: #ccc;
  float: left;
  position: relative;
  top:-5rpx;
  left:30rpx;
}
.dp_text{
  width: 100%;
  text-align: left;
  float: left;
  color: #333;
}
.dp_dz{
  width: 100%;
  float: left;
  font-size: 30rpx;
  color: #ccc;
  margin: 30rpx 0;
}
.xh{
  width: 90%;
  margin: 40rpx auto;
}
.xh text:nth-child(1){
  display: block;
  color: #333;
  font-size: 32rpx;
  font-weight: 800;
}
.yp{
  width: 100%;
  margin: 70rpx auto 0rpx;
}
.yp .yp_a{
  display: block;
  width: 90%;
  margin: 0 auto;
  text-align: left;
  color: #333;
  font-size: 32rpx;
  font-weight: 800;
}
.yp_cont{
  width: 100%;
  background-color: #fff;
  overflow: hidden;
  padding: 40rpx 0;
  margin-bottom: 30rpx;
}
.yp_name{
  width: 90%;
  margin: 0 auto;
  overflow: hidden;
}
.yp_name image{
  width: 50rpx;
  height: 50rpx;
  border-radius: 50%;
  float: left;
}
.yp_name .text{
  font-size: 24rpx;
  color: #ccc;
  float: left;
  margin: 0 20rpx;
}
.yp_b{
  width: 90%;
  margin: 0 auto;
  text-align: left;
}
.yp_title{
  display: block;
  color: #333;
  font-weight: 800;
}
.yp_yp{
  display: block;
  color: #555;
  font-size: 30rpx;
  margin: 20rpx 0;
}
.yp_id{
  display: block;
  color: #888;
  font-size: 28rpx;
}